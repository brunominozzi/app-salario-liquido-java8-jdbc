package com.ccee.projetotestejava.exception;

public class DescontoDaoException extends RuntimeException {

	private static final long serialVersionUID = 1L;

	public DescontoDaoException() {
		super();
	}

	public DescontoDaoException(String message, Throwable cause, boolean enableSuppression,
			boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

	public DescontoDaoException(String message, Throwable cause) {
		super(message, cause);
	}

	public DescontoDaoException(String message) {
		super(message);
	}

	public DescontoDaoException(Throwable cause) {
		super(cause);
	}
	
}